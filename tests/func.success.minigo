func Fibonacci(n int) int {

    if n == 0 {
        return 0;
    };

    a := 0;
    b := 1;

    c := 0;
    for i := 2; i <= n; i = i + 1 {
        c = a + b;
        a = b;
        b = c;
    };
    return c;
};

 func ArrayReversal() {
     var arr [5]int;
     arr[0] = 1;
     arr[1] = 2;
     arr[2] = 3;
     arr[3] = 4;
     arr[4] = 5;
     n := 5;
     for i := 0; i < (n/2); i = i + 1 {
         temp := arr[i];
         idx := n - i - 1;
         arr[i] = arr[idx];
         arr[idx] = temp;
     };

     for i := 0; i < len(arr); i++ {
         printf("arr[%d] = %lld\n", i, arr[i]);
     };
 };

